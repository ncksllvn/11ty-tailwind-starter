<head>
  <meta charset="utf-8">
  <title>{{ title }}</title>


  <meta name="description" content="">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">

  <meta property="og:title" content="{{ title }} | {{ pkg.websiteMetadata.title }}">
  <meta property="og:url" content="{{ page.url | url }}">

  <meta property="og:type" content="website">
  <meta property="og:image" content="{{ "/tile.png" | url }}">

  <link rel="manifest" href="{{ "/site.webmanifest" | url }}">
  <link rel="apple-touch-icon" href="{{ "/icon.png" | url }}">

  <link rel="stylesheet" href="{{ "/style/main.css" | url }}">
  <meta name="theme-color" content="{{ pkg.websiteMetadata.themeColor }}">
</head>
