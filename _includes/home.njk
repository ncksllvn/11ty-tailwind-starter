<html>
  {% include 'head.njk' %}
<body>
  {{ content | safe }}

  <h2>Stories</h2>
  <ul>
  {%- for story in collections.stories -%}
    <li>
      <a href="{{ story.url }}">{{ story.data.title }}</a>
    </li>
  {%- endfor -%}
  </ul>

  <h2>Projects</h2>
  <ul>
  {%- for project in collections.projects -%}
    <li>
      <a href="{{ project.url }}">{{ project.data.title }}</a>
    </li>
  {%- endfor -%}
  </ul>
</body>
</html>
